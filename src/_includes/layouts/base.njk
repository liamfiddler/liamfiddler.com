<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preload" href="/resources/script.js" as="script" crossorigin>
    <link rel="stylesheet" href="/style.css">
    <title>{{ title }}</title>
    <meta name="description" content="{{ description }}">
  </head>
  <body>
    <a class="visually-hidden" href="#main-content">Skip to content</a>
    <header role="banner">
      <div class="contain">
        <a href="/" {{ 'aria-current=page' if page.url == '/' | safe }} class="title">
          <span class="visually-hidden">{{ 'Back to homepage' if page.url != '/' else 'Homepage' }}</span>
          <img src="/resources/avatar.png" aria-hidden="true" />
          <span aria-hidden="true">Liam Fiddler</span>
        </a>
        <nav>
          <a href="/projects" {{ 'aria-current=page' if page.url == '/projects/' | safe }}>Side Projects</a>
          <a href="/posts" {{ 'aria-current=page' if page.url == '/posts/' | safe }}>Posts</a>
          <a href="/search" {{ 'aria-current=page' if page.url == '/search/' | safe }}>
            <svg role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48">
              <g transform="translate(0 -1004.36)" fill="none">
                <circle cx="20" cy="1024.36" r="9.87" stroke-width="4.27" />
                <path d="M37.87 1042.23l-7.6-7.6" stroke-width="6.67" />
                <path d="M26.67 1031.03l5.33 5.33" stroke-width="2.67" />
              </g>
            </svg>
            <span class="visually-hidden">Search</span>
          </a>
        </nav>
      </div>
    </header>
    <main id="main-content">
      <div class="fix-swup-bug" class="transition-content">
        {% block content %}
        {% endblock content %}
      </div>
    </main>
    <footer>
      <div class="contain">
        <a href="https://twitter.com/liamfiddler">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
